<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://apple.com/itunes/importer" xmlns:importer="http://apple.com/itunes/importer">
  <xs:element name="package" type="importer:PackageType"/>
  <xs:complexType name="PackageType">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:metadata_token"/>
      <xs:element ref="importer:comments"/>
      <xs:element ref="importer:language"/>
      <xs:element ref="importer:provider"/>
      <xs:choice>
        <xs:element ref="importer:album"/>
        <xs:element ref="importer:tracks"/>
        <xs:element ref="importer:assets"/>
        <xs:element ref="importer:video"/>
        <xs:element ref="importer:artist"/>
        <xs:element ref="importer:clone_assets"/>
      </xs:choice>
    </xs:choice>
    <xs:attribute name="version"  type="importer:PackageVersion"/>
    <xs:attribute name="generator" type="importer:String255"/>
    <xs:attribute name="generator_version" type="importer:String255"/>
  </xs:complexType>
  <xs:element name="metadata_token" type="xs:string"/>
  <xs:element name="comments" type="xs:string"/>
  <xs:element name="language" type="importer:LanguageType"/>
  <xs:element name="provider" type="importer:ProviderType"/>
  <xs:element name="album" type="importer:Album"/>
  <xs:element name="tracks" type="importer:TrackList"/>
  <xs:element name="assets" type="importer:AssetList"/>
  <xs:element name="video" type="importer:Video"/>
  <xs:element name="artist" type="importer:ArtistType"/>
  <xs:element name="clone_assets" type="importer:CloneAssetList"/>
  <xs:simpleType name="PackageVersion">
    <xs:restriction base="xs:token">
      <xs:pattern value="(music|musicvideosingle)\d+(\.\d+)*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Weekday">
    <xs:restriction base="xs:token">
      <xs:enumeration value="Monday"/>
      <xs:enumeration value="Tuesday"/>
      <xs:enumeration value="Wednesday"/>
      <xs:enumeration value="Thursday"/>
      <xs:enumeration value="Friday"/>
      <xs:enumeration value="Saturday"/>
      <xs:enumeration value="Sunday"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Time">
    <xs:restriction base="importer:Formatted24HourTime"/>
  </xs:simpleType>
  <xs:attributeGroup name="Time">
    <xs:attribute name="zone" >
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="central"/>
          <xs:enumeration value="eastern"/>
          <xs:enumeration value="local"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:complexType name="AssetList">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:asset"/>
      <xs:element ref="importer:accessibility_info"/>
    </xs:choice>
    <xs:attribute name="container_id" type="importer:VendorIdType"/>
    <xs:attribute name="media_type">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="video"/>
          <xs:enumeration value="playlist"/>
          <xs:enumeration value="extra_video_item"/>
          <xs:enumeration value="gallery_item_video"/>
          <xs:enumeration value="gallery_item_image"/>
          <xs:enumeration value="gallery_item_audio"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="vendor_id" type="importer:VendorIdType"/>
  </xs:complexType>
  <xs:element name="asset" type="importer:AssetItem"/>
  <xs:element name="accessibility_info" type="importer:AccessibilityInfo"/>
  <xs:complexType name="CloneAssetList">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:clone_asset"/>
      <xs:element ref="importer:clone_chapters"/>
    </xs:choice>
    <xs:attribute name="container_id" type="importer:VendorIdType"/>
    <xs:attribute name="media_type">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="video"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="origin_provider"  type="importer:ProviderType"/>
    <xs:attribute name="origin_vendor_id"  type="importer:VendorIdType"/>
    <xs:attribute name="target_vendor_id" type="importer:VendorIdType"/>
  </xs:complexType>
  <xs:element name="clone_asset" type="importer:CloneAssetItem"/>
  <xs:element name="clone_chapters" type="importer:FormattedBoolean"/>
  <xs:complexType name="Video">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:read_only_info"/>
      <xs:element ref="importer:original_spoken_locale"/>
      <xs:element name="phonetic_title" type="importer:NonEmptyTokenString"/>
      <xs:element ref="importer:vendor_offer_code"/>
      <xs:element ref="importer:publishers"/>
      <xs:element ref="importer:volume_number"/>
      <xs:element ref="importer:artists"/>
      <xs:element name="artwork_files" type="importer:ArtworkFilesList"/>
      <xs:element ref="importer:copyright_cline"/>
      <xs:element ref="importer:copyright_pline"/>
      <xs:element ref="importer:explicit_content"/>
      <xs:element ref="importer:lyrics"/>
      <xs:element ref="importer:grid"/>
      <xs:element ref="importer:isrc"/>
      <xs:element ref="importer:eidr"/>
      <xs:element ref="importer:preorder_type"/>
      <xs:element ref="importer:preview"/>
      <xs:element ref="importer:release_date"/>
      <xs:element name="title" type="xs:string"/>
      <xs:element name="title_version" type="xs:string"/>
      <xs:element ref="importer:track_number"/>
      <xs:element ref="importer:upc"/>
      <xs:element ref="importer:vendor_id"/>
      <xs:element ref="importer:xid"/>
      <xs:element ref="importer:assets"/>
      <xs:element ref="importer:clone_assets"/>
      <xs:element ref="importer:share_assets"/>
      <xs:element ref="importer:genres"/>
      <xs:element name="label_name" type="importer:LabelNameType"/>
      <xs:element ref="importer:products"/>
      <xs:element ref="importer:cue_sheet"/>
      <xs:element name="locales">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" name="locale" type="importer:VideoLocale"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element ref="importer:chapters"/>
      <xs:element ref="importer:accessibility_info"/>
      <xs:element ref="importer:previews"/>
      <xs:choice>
        <xs:element name="long_description" type="importer:String4000"/>
        <xs:element ref="importer:synopsis"/>
      </xs:choice>
      <xs:element ref="importer:extra"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="read_only_info" type="importer:ReadOnlyInfo"/>
  <xs:element name="original_spoken_locale" type="importer:LanguageType"/>
  <xs:element name="vendor_offer_code" type="xs:string"/>
  <xs:element name="publishers" type="importer:PublisherList"/>
  <xs:element name="volume_number" type="importer:FormattedPositiveInteger"/>
  <xs:element name="artists" type="importer:ArtistList"/>
  <xs:element name="copyright_cline" type="xs:string"/>
  <xs:element name="copyright_pline" type="xs:string"/>
  <xs:element name="explicit_content">
    <xs:simpleType>
      <xs:union memberTypes="importer:ParentalAdvisoryType">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="lyrics">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="importer:LyricsType"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="grid" type="xs:string"/>
  <xs:element name="isrc" type="importer:Isrc"/>
  <xs:element name="eidr" type="importer:Eidr"/>
  <xs:element name="preorder_type" type="importer:PreOrder"/>
  <xs:element name="preview">
    <xs:complexType mixed="true">
      <xs:attributeGroup ref="importer:VideoPreviewType"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="release_date">
    <xs:simpleType>
      <xs:union memberTypes="importer:FormattedDate">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="track_number" type="importer:FormattedPositiveInteger"/>
  <xs:element name="upc" type="importer:Upc"/>
  <xs:element name="vendor_id" type="importer:VendorIdType"/>
  <xs:element name="xid" type="importer:Xid"/>
  <xs:element name="share_assets">
    <xs:complexType>
      <xs:attributeGroup ref="importer:ShareAssets"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="genres" type="importer:GenreList"/>
  <xs:element name="products" type="importer:ProductList"/>
  <xs:element name="cue_sheet" type="importer:CueSheet"/>
  <xs:element name="chapters" type="importer:ChapterList"/>
  <xs:element name="previews" type="importer:PreviewDefinitionList"/>
  <xs:element name="synopsis" type="importer:String4000"/>
  <xs:element name="extra" type="importer:Extra"/>
  <xs:complexType name="AccessibilityInfo">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:read_only_info"/>
      <xs:element ref="importer:accessibility"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="accessibility">
    <xs:complexType>
      <xs:attributeGroup ref="importer:AccessibilityDetail"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="AccessibilityDetail">
    <xs:attribute name="role"  type="importer:AccessibilityRole"/>
    <xs:attribute name="available"  type="xs:boolean"/>
    <xs:attribute name="locale" type="importer:Locale"/>
    <xs:attribute name="reason_code" type="importer:AccessibilityReasonCode"/>
  </xs:attributeGroup>
  <xs:simpleType name="AccessibilityRole">
    <xs:restriction base="xs:token">
      <xs:enumeration value="captions"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AccessibilityReasonCode">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <!-- Begin film and tv-specific definitions -->
  <xs:attributeGroup name="ShareAssets">
    <xs:attribute name="provider" type="importer:ProviderType"/>
    <xs:attribute name="vendor_id" type="importer:VendorIdType"/>
    <xs:attribute name="apple_id" type="importer:FormattedPositiveInteger"/>
  </xs:attributeGroup>
  <xs:complexType name="VideoLocale">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:designated_fallback"/>
      <xs:element name="title" type="xs:string"/>
      <xs:element name="title_version" type="xs:string"/>
      <xs:element ref="importer:short_description"/>
      <xs:choice>
        <xs:element name="long_description" type="xs:string"/>
        <xs:element ref="importer:synopsis"/>
      </xs:choice>
      <xs:element name="phonetic_title" type="xs:string"/>
    </xs:choice>
    <xs:attribute name="name"  type="importer:LanguageType"/>
  </xs:complexType>
  <xs:element name="designated_fallback" type="importer:FormattedBoolean"/>
  <xs:element name="short_description" type="importer:String255"/>
  <xs:group name="VideoRegion">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="importer:territory"/>
        <xs:element ref="importer:theatrical_release_date"/>
        <xs:element ref="importer:release_date"/>
        <xs:element ref="importer:copyright_pline"/>
        <xs:element ref="importer:copyright_cline"/>
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:element name="territory" type="importer:Territory"/>
  <xs:element name="theatrical_release_date">
    <xs:simpleType>
      <xs:union memberTypes="importer:FormattedDate">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:complexType name="CueSheet">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="importer:cue"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="cue" type="importer:Cue"/>
  <xs:complexType name="Cue">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:sequence_number"/>
      <xs:element ref="importer:time"/>
      <xs:element name="title" type="xs:string"/>
      <xs:element ref="importer:artists"/>
      <xs:element ref="importer:track_id"/>
      <xs:element ref="importer:isrc"/>
      <xs:element name="iswc" type="xs:string"/>
      <xs:element ref="importer:copyright_cline"/>
      <xs:element ref="importer:copyright_pline"/>
      <xs:element ref="importer:usage"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="sequence_number" type="importer:FormattedPositiveInteger"/>
  <xs:element name="time" type="importer:CueTime"/>
  <xs:element name="track_id" type="importer:FormattedPositiveInteger"/>
  <xs:element name="usage" type="importer:CueUsage"/>
  <xs:simpleType name="CueUsage">
    <xs:restriction base="xs:token">
      <xs:pattern value="([Bb][Aa][Cc][Kk][Gg][Rr][Oo][Uu][Nn][Dd])|([Ff][Ee][Aa][Tt][Uu][Rr][Ee][Dd])|([Ss][Ii][Gg][Nn][Aa][Tt][Uu][Rr][Ee])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CueTime">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:start_timecode"/>
      <xs:element ref="importer:duration"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="start_timecode" type="importer:FormattedTime"/>
  <xs:element name="duration" type="importer:FormattedTime"/>
  <!-- VideoAdvisory is only supported by TV -->
  <!-- FilmRating is supported by Film and TV -->
  <xs:group name="MovieParticipantLocale">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="importer:designated_fallback"/>
        <xs:element name="display_name" type="importer:String250"/>
        <xs:element ref="importer:character_name"/>
        <xs:element name="phonetic_name" type="importer:String250"/>
        <xs:element name="description">
          <xs:complexType mixed="true">
            <xs:attributeGroup ref="importer:Description"/>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:element name="character_name" type="importer:String250"/>
  <xs:attributeGroup name="MovieParticipantLocale">
    <xs:attribute name="name"  type="importer:Locale"/>
  </xs:attributeGroup>
  <!-- End film and tv-specific definitions -->
  <xs:complexType name="AssetItem">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:read_only_info"/>
      <xs:element ref="importer:territories"/>
      <xs:element ref="importer:data_file"/>
      <xs:element ref="importer:chapters"/>
      <xs:element ref="importer:production_still_image_time"/>
    </xs:choice>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="full"/>
          <xs:enumeration value="preview"/>
          <xs:enumeration value="artwork"/>
          <xs:enumeration value="head"/>
          <xs:enumeration value="tail"/>
          <xs:enumeration value="timeline"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="display_target"/>
  </xs:complexType>
  <xs:element name="territories">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="importer:territory"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="data_file" type="importer:AssetFile"/>
  <xs:element name="production_still_image_time" type="importer:ProductionStillImageTime"/>
  <xs:simpleType name="PreviewImageTime">
    <xs:restriction base="importer:FormattedTime"/>
  </xs:simpleType>
  <xs:attributeGroup name="PreviewImageTime">
    <xs:attribute name="format"/>
  </xs:attributeGroup>
  <xs:complexType name="ProductionStillImageTime">
    <xs:simpleContent>
      <xs:extension base="importer:FormattedTime">
        <xs:attribute name="format"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="CloneAssetItem">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:read_only_info"/>
      <xs:element ref="importer:territories"/>
      <xs:element ref="importer:clone_data_file"/>
    </xs:choice>
    <xs:attribute name="type">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="full"/>
          <xs:enumeration value="preview"/>
          <xs:enumeration value="artwork"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="clone_data_file">
    <xs:complexType>
      <xs:attributeGroup ref="importer:CloneAssetFile"/>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="Album">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:read_only_info"/>
      <xs:element ref="importer:galleries"/>
      <xs:element name="locales" type="importer:LocalizationList"/>
      <xs:element ref="importer:album_type"/>
      <xs:element ref="importer:vendor_id"/>
      <xs:element ref="importer:grid"/>
      <xs:element ref="importer:preorder_grid"/>
      <xs:element name="title" type="xs:string"/>
      <xs:element name="title_version" type="xs:string"/>
      <xs:element name="phonetic_title" type="xs:string"/>
      <xs:element ref="importer:copyright_pline"/>
      <xs:element ref="importer:copyright_cline"/>
      <xs:element name="artwork_files">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="importer:file"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element ref="importer:liner_notes"/>
      <xs:element name="description" type="importer:String4000"/>
      <xs:element ref="importer:volume_count"/>
      <xs:element ref="importer:track_count"/>
      <xs:element ref="importer:products"/>
      <xs:element ref="importer:preorder_previews"/>
      <xs:element ref="importer:upc"/>
      <xs:element ref="importer:preorder_upc"/>
      <xs:element ref="importer:original_release_date"/>
      <xs:element name="label_name" type="importer:NonEmptyTokenString"/>
      <xs:element ref="importer:genres"/>
      <xs:element ref="importer:artists"/>
      <xs:element ref="importer:tracks"/>
      <xs:element ref="importer:vendor_offer_code"/>
      <xs:element ref="importer:index_alias_terms"/>
      <xs:element ref="importer:hero_image"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="galleries" type="importer:GalleryList"/>
  <xs:element name="album_type" type="importer:AlbumType"/>
  <xs:element name="preorder_grid" type="xs:string"/>
  <xs:element name="file" type="importer:File"/>
  <xs:element name="liner_notes" type="xs:string"/>
  <xs:element name="volume_count" type="importer:FormattedPositiveInteger"/>
  <xs:element name="track_count" type="importer:FormattedPositiveInteger"/>
  <xs:element name="preorder_previews" type="importer:FormattedBoolean"/>
  <xs:element name="preorder_upc" type="importer:Upc"/>
  <xs:element name="original_release_date">
    <xs:simpleType>
      <xs:union memberTypes="importer:FormattedDate">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:length value="0"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
    </xs:simpleType>
  </xs:element>
  <xs:element name="index_alias_terms" type="xs:string"/>
  <xs:element name="hero_image" type="importer:HeroImage"/>
  <xs:simpleType name="AlbumType">
    <xs:restriction base="xs:token">
      <xs:enumeration value="album"/>
      <xs:enumeration value="audiobook"/>
      <xs:enumeration value="ringtone"/>
      <xs:enumeration value="iosaudiotone"/>
      <xs:enumeration value="Album"/>
      <xs:enumeration value="Audiobook"/>
      <xs:enumeration value="AudioBook"/>
      <xs:enumeration value="Ringtone"/>
      <xs:enumeration value="iOSAudioTone"/>
      <xs:enumeration value="MovieBundle"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="TrackList">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:album_upc"/>
      <xs:choice>
        <xs:element name="track" type="importer:Track"/>
        <xs:element ref="importer:video"/>
        <xs:element ref="importer:work"/>
        <xs:element ref="importer:track_group"/>
        <xs:element ref="importer:booklet"/>
        <xs:element ref="importer:extra"/>
      </xs:choice>
      <xs:element ref="importer:bonus_material"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="album_upc" type="importer:Upc"/>
  <xs:element name="work" type="importer:WorkType"/>
  <xs:element name="track_group" type="importer:TrackGroup"/>
  <xs:element name="booklet" type="importer:Booklet"/>
  <xs:element name="bonus_material" type="importer:BonusMaterialItem"/>
  <xs:group name="BonusMaterialList">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="importer:bonus_material"/>
    </xs:sequence>
  </xs:group>
  <xs:complexType name="BonusMaterialItem">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:vendor_identifier"/>
      <xs:element ref="importer:file"/>
      <xs:element name="track" type="importer:FormattedPositiveInteger"/>
      <xs:element ref="importer:preorder_type"/>
      <xs:element ref="importer:volume"/>
      <xs:element ref="importer:bonus_material_name"/>
      <xs:element ref="importer:copyright"/>
    </xs:choice>
  </xs:complexType>
  <xs:element name="vendor_identifier" type="importer:VendorIdType"/>
  <xs:element name="volume" type="importer:FormattedPositiveInteger"/>
  <xs:element name="bonus_material_name" type="importer:NonEmptyTokenString"/>
  <xs:element name="copyright" type="xs:string"/>
  <xs:complexType name="WorkType">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:products"/>
      <xs:element name="track" type="importer:Track"/>
    </xs:choice>
    <xs:attribute name="name" />
  </xs:complexType>
  <xs:complexType name="TrackGroup">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="importer:vendor_id"/>
      <xs:element ref="importer:group_name"/>
      <xs:element name="artwork_files" type="importer:ArtworkFilesList"/>
      <xs:element ref="importer:products"/>
      <xs:choice>
        <xs:element name="track" type="importer:Track"/>
        <xs:element ref="importer:video"/>
        <xs:element ref="importer:booklet"/>
        <xs:element ref="importer:extra"/>
      </xs:choice>
    </xs:choice>
  </xs:complexType>
  <xs:element name="group_name" type="importer:NonEmptyTokenString"/>
  <xs:complexType name="ArtworkFilesList">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" ref="importer:file"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PreviewDefinitionList">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="importer:preview"/>
    </xs:sequence>
  </xs:complexType>
  <xs:attributeGroup name="VideoPreviewType">
    <xs:attribute name="starttime">
